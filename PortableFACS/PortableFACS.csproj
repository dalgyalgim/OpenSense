<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>netstandard2.1</TargetFramework>
		<LangVersion>11.0</LangVersion>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<RootNamespace>PortableFACS</RootNamespace>
		<AssemblyName>PortableFACS</AssemblyName>
		<EmbedAllSources>true</EmbedAllSources>
		<Version>1.0.0</Version>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.ML.OnnxRuntime" Version="1.14.1" />
		<PackageReference Include="System.Numerics.Tensors" Version="0.1.0" />
	</ItemGroup>

	<ItemGroup>
		<None Update="PortableFACS*.onnx">
			<CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
		</None>
	</ItemGroup>

</Project>
