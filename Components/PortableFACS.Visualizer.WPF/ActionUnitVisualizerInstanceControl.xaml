<UserControl
    x:Class="OpenSense.WPF.Components.PortableFACS.ActionUnitVisualizerInstanceControl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:OpenSense.WPF.Components.PortableFACS"
    mc:Ignorable="d"
    d:DesignHeight="450"
    d:DesignWidth="800">
    <Grid>
        <TextBlock
            Grid.Row="0"
            Grid.Column="1"
            Text="{Binding Text}" />
    </Grid>
</UserControl>
